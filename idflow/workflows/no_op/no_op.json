{
  "name": "no_op_stage",
  "version": 1,
  "schemaVersion": 2,
  "inputParameters": ["docId", "anything"],
  "tasks": [
    {
      "name": "no_op_inline",
      "taskReferenceName": "noop",
      "type": "INLINE",
      "inputParameters": {
        "evaluatorType": "javascript",
        "now": "${workflow.startTime}",
        "payload": "${workflow.input}",
        "expression": "(function(){ return { ok: true, received: $.payload, startedAtEpochMs: $.now }; })()"
      }
    }
  ],
  "outputParameters": {
    "ok": "${noop.output.result.ok}",
    "received": "${noop.output.result.received}",
    "startedAtEpochMs": "${noop.output.result.startedAtEpochMs}"
  }
}
