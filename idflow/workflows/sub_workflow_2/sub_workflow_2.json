{
  "name": "sub_workflow_2",
  "version": 2,
  "schemaVersion": 2,
  "inputParameters": [
    "docId",
    "stageName",
    "stageCounter"
  ],
  "tasks": [
    {
      "name": "NOOP",
      "taskReferenceName": "noop2",
      "type": "NOOP",
      "inputParameters": {
        "message": "Sub-workflow 2 executed"
      }
    },
    {
      "name": "create_test_doc",
      "taskReferenceName": "create_test_doc",
      "type": "INLINE",
      "inputParameters": {
        "evaluatorType": "javascript",
        "expression": "({ doc: { title: 'Hello from sub_workflow_2', body: 'This is a test document created by sub_workflow_2' } })"
      }
    }
  ],
  "outputParameters": {
    "docId": "${workflow.input.docId}",
    "stageName": "${workflow.input.stageName}",
    "stageCounter": "${workflow.input.stageCounter}",
    "subWorkflow": "sub_workflow_2",
    "status": "completed",
    "doc": "${create_test_doc.output.result.doc}"
  },
  "ownerEmail": "idflow@example.com"
}
